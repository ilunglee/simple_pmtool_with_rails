<div class="container">
<h1>default home</h1>
<% @projects.each do |project| %>
  <div class="row">
    <div class="col-md-9"><h2><%= link_to project.title, project_path(project.id) %></h2></div>
    <div class="col-md-3 heading">
      <%= link_to "Edit", edit_project_path(project), class: "btn btn-default pull-right" %>
      <%= button_to 'Delete', project,
          method: :delete, class: "btn btn-danger pull-right", confirm: 'Are you sure you want to delete this project?' %>
    </div>
  </div>
  <div class="row">
    <p class="col-md-12"> <%= project.created_at.strftime("%Y-%b-%d") %></p>
    <p class="col-md-12"> <%= project.description %> </p>
  </div>
  <hr>
<% end %>
</div>
